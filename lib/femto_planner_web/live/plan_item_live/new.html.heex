<.form :let={f} for={@changeset} phx-change="change" phx-submit="create">
  <.fields f={f} />
  <div class="flex justify-end p-4">
    <.link patch={~p(/plan_items)} class="btn btn-neutral mr-2">
      <%= dgettext("plan_item_live", "Cancel") %>
    </.link>
    <button class="btn btn-accent">
      <%= dgettext("plan_item_live", "Create") %>
    </button>
  </div>
</.form>
